<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="stylesheets/projects.css">
  <link href="https://fonts.googleapis.com/css?family=Exo:200|Raleway:100,100i,200|Roboto:300,300i,400,400i,500,500i,700,700i,900,900i" rel="stylesheet">
  <title>Loomx</title>
</head>
<body style = "margin:0" class="no-touch">
  <div class="container">
    <nav class="topnav">
      <ul>
        <li><a class="active" id="home" href="/">Home</a></li>
        <li><a href="/projects">Projects</a></li>
        <li><a href="/about">About</a></li>
        <li><a href="/contact">Contact</a></li>
      </ul>
    </nav>
    <div id="section_one">
      <h1 class="title">Projects</h1>
    </div>
    <div class="wrap">
      <div class="box">
        <div class="boxInner">
          <img src="images/delta/delta_cover.jpeg">
          <div class="titleBox">Fraternal Organization</div>
        </div>
      </div>
      <div class="box">
        <div class="boxInner">
          <img src="images/cityofbing/cityofbing_cover.jpg">
          <div class="titleBox">City of Binghamton</div>
        </div>
      </div>
      <div class="box">
        <div class="boxInner">
          <img src="images/colonial/colonial_cover.jpg">
          <div class="titleBox">The Colonial</div>
        </div>
      </div>
    </div>

  </div>
  <script src="/scripts/interactive.js"></script>
  <script type="text/javascript" src="http://code.jquery.com/jquery-1.8.3.js"></script>
  <script type="text/javascript">
  $(function(){
    // See if this is a touch device
    if ('ontouchstart' in window)
    {
      // Set the correct body class
      $('body').removeClass('no-touch').addClass('touch');

      // Add the touch toggle to show text
      $('div.boxInner img').click(function(){
        $(this).closest('.boxInner').toggleClass('touchFocus');
      });
    }
  });
</script>
</body>
</html>
